<?xml version='1.0' encoding='UTF-8'?>
<Settings>
	<Module name="FIFE">
		<Setting name="WindowTitle" type="str"> Awooo~ </Setting>
		<Setting name="WindowIcon" type="str"> </Setting>
		<Setting name="ScreenResolution" type="str">1024x768</Setting>
		<Setting name="FullScreen" type="bool"> False </Setting>

		<Setting name="RenderBackend" type="str"> OpenGL </Setting>
		<Setting name="Lighting" type="int"> 1 </Setting>
		<Setting name="BitsPerPixel" type="int"> 0 </Setting>
		<Setting name="SDLRemoveFakeAlpha" type="bool"> True </Setting>
		<Setting name="GLCompressImages" type="bool"> False </Setting>
		<Setting name="GLUseFrameBuffer" type="bool"> False </Setting>
		<Setting name="GLUseNPOT" type="bool"> True </Setting>

		<Setting name="PlaySounds" type="bool"> True </Setting>
		<Setting name="InitialVolume" type="float"> 10.0 </Setting>

		<Setting name="MouseSensitivity" type="float"> 0.0 </Setting>
		<Setting name="MouseAcceleration" type="bool"> False </Setting>
		<Setting name="NativeImageCursor" type="bool"> False </Setting>

		<Setting name="Font" type="str"> fonts/FreeSans.ttf </Setting>
		<Setting name="FontGlyphs" strip="0" type="str"> abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789.,!?-+/():;%&amp;`'*#=[]\"</Setting>
		<Setting name="DefaultFontSize" type="int"> 16 </Setting>

		<Setting name="LogModules" type="list"> all </Setting>
		<Setting name="LogLevelFilter" type="int"> 1 </Setting>
		<Setting name="LogToPrompt" type="bool"> True </Setting>
		<Setting name="LogToFile" type="bool"> True </Setting>

		<Setting name="PychanDebug" type="bool"> False </Setting>

		<Setting name="FrameLimitEnabled" type="bool"> False </Setting>
		<Setting name="FrameLimit" type="int"> 60 </Setting>
		<Setting name="VSync" type="bool"> True </Setting>

		<Setting name="GLUseMipmapping" type="bool"> True </Setting>
		<Setting name="GLTextureFiltering" type="str"> None </Setting>
	</Module>
	<Module name="CEGUI">
		<Setting name="schemes" type="str"> gui/schemes/ </Setting>
	</Module>
	<Module name="hotkeys">
		<Setting name="Rotate Clockwise" type="unicode">KP_MULTIPLY</Setting>
		<Setting name="Rotate Counterclockwise" type="unicode">KP_DIVIDE</Setting>
		<Setting name="Move NE" type="unicode">UP</Setting>
		<Setting name="Move NW" type="unicode">LEFT</Setting>
		<Setting name="Move SW" type="unicode">DOWN</Setting>
		<Setting name="Move SE" type="unicode">RIGHT</Setting>
	</Module>
</Settings>
